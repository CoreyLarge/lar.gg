{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "jsx": "react",
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@common/*": [
        "./src/components/common/*"
      ]
    },
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules"
  ]
}
